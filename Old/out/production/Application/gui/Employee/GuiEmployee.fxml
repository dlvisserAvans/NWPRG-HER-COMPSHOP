<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Employee.ControllerEmployee">
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <ComboBox fx:id="ComboBoxDevices" prefHeight="25.0" prefWidth="272.0" />
            <Button fx:id="btnAddToCart" mnemonicParsing="false" onAction="#onButtonAddToCartClick" text="ADD">
               <font>
                  <Font name="Calibri" size="18.0" />
               </font>
            </Button>
         </children></VBox>
   </center>
   <top>
      <Label fx:id="labelText" alignment="BOTTOM_CENTER" text="Customer Portal" BorderPane.alignment="CENTER">
         <font>
            <Font name="Calibri" size="36.0" />
         </font>
         <padding>
            <Insets top="10.0" />
         </padding>
      </Label>
   </top>
   <bottom>
      <Button fx:id="btnRefresh" mnemonicParsing="false" onAction="#onButtonRefreshClick" text="Refresh" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
